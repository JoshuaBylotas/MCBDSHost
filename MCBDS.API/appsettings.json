{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Runner": {
    "ExePath": "/app/Binaries/bedrock_server",
    "LogFilePath": "/app/logs/runner.log"
  },
  "Backup": {
    "FrequencyMinutes": 30,
    "BackupDirectory": "/app/backups",
    "MaxBackupsToKeep": 30
  }
}